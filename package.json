{
  "name": "node-aws-travis-playground",
  "version": "4.2.0",
  "description": "",
  "main": "app.js",
  "private": true,
  "scripts": {
    "clean": "rm -rf lib",
    "lint": "eslint .",
    "test": "mocha --compilers js:babel/register",
    "start": "node lib/server",
    "build": "babel src -d lib --experimental",
    "coverage": "istanbul cover --report lcovonly _mocha -- --compilers js:babel/register",
    "prebuild": "npm run clean",
    "precoverage": "rm -rf ./coverage",
    "postcoverage": "if [ -n \"$CI\" ]; then cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js; fi",
    "pretest": "npm run lint",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "docker-build": "docker build --no-cache -t liddellj/my-app:$npm_package_version .",
    "docker-run": "docker run -t -i -P liddellj/my-app:$npm_package_version",
    "docker-push": "docker push liddellj/my-app:$npm_package_version",
    "docker-aws-version": "sed -i -e s/my-app/my-app:$npm_package_version/g Dockerrun.aws.json",
    "watch": "babel src -d lib --experimental -w",
    "dev-server": "node lib/webpack",
    "server": "nodemon --debug lib/server",
    "start-dev": "npm run watch & npm run dev-server & npm run server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liddellj/node-aws-travis-playground.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/liddellj/node-aws-travis-playground/issues"
  },
  "homepage": "https://github.com/liddellj/node-aws-travis-playground#readme",
  "devDependencies": {
    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "chai": "^3.2.0",
    "coveralls": "^2.11.4",
    "eslint": "^1.1.0",
    "eslint-plugin-react": "^3.2.3",
    "istanbul": "^0.3.18",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "0.0.2",
    "nodemon": "^1.4.1",
    "react-hot-loader": "^1.2.8",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1"
  },
  "dependencies": {
    "babel": "^5.8.21",
    "express": "^4.13.3",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "jade": "^1.11.0"
  }
}
