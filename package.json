{
  "name": "node-aws-travis-playground",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "private": true,
  "scripts": {
    "clean": "rm -rf lib",
    "watch": "babel src -d lib --experimental -w",
    "dev-server": "node lib/webpack",
    "server": "nodemon --debug lib/server",
    "start-dev": "npm run watch & npm run dev-server & npm run server",
    "test": "eslint src && mocha --compilers js:babel/register -R spec tests/*Specs.js",
    "start": "node lib/server",
    "build": "npm run clean && babel src -d lib --experimental",
    "docker-build": "docker build --no-cache -t liddellj/myimage .",
    "docker-run": "docker run -t -i -P liddellj/myimage",
    "coverage": "istanbul cover --report lcovonly _mocha -- --compilers js:babel/register -R spec tests/*Specs.js && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liddellj/node-aws-travis-playground.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/liddellj/node-aws-travis-playground/issues"
  },
  "homepage": "https://github.com/liddellj/node-aws-travis-playground#readme",
  "devDependencies": {
    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "chai": "^3.2.0",
    "coveralls": "^2.11.4",
    "eslint": "^1.1.0",
    "eslint-plugin-react": "^3.2.3",
    "istanbul": "^0.3.18",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "0.0.2",
    "nodemon": "^1.4.1",
    "react-hot-loader": "^1.2.8",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1"
  },
  "dependencies": {
    "babel": "^5.8.21",
    "express": "^4.13.3",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "jade": "^1.11.0"
  }
}
